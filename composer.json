{
    "name": "shira/php-ext-usage",
    "description": "Find out which PHP extensions your code uses",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "ext-mbstring": "*",
        "nikic/php-parser": "^4.0",
        "symfony/console": "^4.1",
        "kuria/enum": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Shira\\PhpExtUsage\\": "src"
        }
    },
    "bin": [
        "bin/php-ext-usage"
    ],
    "config": {
        "platform": {
            "php": "7.1.999"
        }
    }
}